{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-clientsample",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/SampleCode/WorkWallet.BI.ClientSample/WorkWallet.BI.ClientSample.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "build-clientdatabasedeploy",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/SampleCode/WorkWallet.BI.ClientDatabaseDeploy/WorkWallet.BI.ClientDatabaseDeploy.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "build-solution",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/SampleCode/WorkWallet.BI.Client.sln",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "clean-solution",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/SampleCode/WorkWallet.BI.Client.sln"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "restore-solution",
            "command": "dotnet",
            "type": "process",
            "args": [
                "restore",
                "${workspaceFolder}/SampleCode/WorkWallet.BI.Client.sln"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "run-clientsample",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/SampleCode/WorkWallet.BI.ClientSample/WorkWallet.BI.ClientSample.csproj"
            ],
            "group": "test",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "new"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "run-clientdatabasedeploy",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/SampleCode/WorkWallet.BI.ClientDatabaseDeploy/WorkWallet.BI.ClientDatabaseDeploy.csproj"
            ],
            "group": "test",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "new"
            },
            "problemMatcher": "$msCompile"
        }
    ]
}
